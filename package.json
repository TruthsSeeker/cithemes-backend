{
  "name": "srcplz-backend",
  "version": "1.0.0",
  "description": "",
  "main": "dist/index.js",
  "scripts": {
    "start:dev": "nodemon dist/index.js",
    "build:dev": "tsc --watch --preserveWatchOutput",
    "migrate:latest": "knex migrate:latest --knexfile src/db/knexfile.ts",
    "migrate:rollback": "knex migrate:rollback --knexfile src/db/knexfile.ts",
    "migrate:make": "knex --knexfile src/db/knexfile.ts migrate:make",
    "dev": "concurrently \"yarn build:dev\" \"yarn start:dev\""
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
