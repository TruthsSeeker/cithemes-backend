    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.11",
    "@types/express-jwt": "^6.0.0",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/knex": "^0.16.1",
    "@types/node": "^14.14.21",
    "@types/sqlite3": "^3.1.6",
    "@types/uuid": "^8.3.0",
    "bcrypt": "^5.0.0",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
